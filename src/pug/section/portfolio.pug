        .wrap
            ul
                -
                    var projects = [
                        {
                            class: 'ambrosus',
                            name: 'Ambrosus',
                            symbol: '/assets/portfolio/ambrosus.svg',
                            url:'https://ambrosus.com/'
                        },
                        {
                            class: 'eos',
                            name: 'EOS',
                            symbol: '/assets/portfolio/eos.png',
                            url:'https://eos.io/'
                        },
                        {
                            class: 'gonetwork',
                            name: 'GoNetwork',
                            symbol: '/assets/portfolio/gonet.svg',
                            url:'https://gonetwork.co/'
                        },
                        {
                            class: 'komodo',
                            name: 'Komodo',
                            symbol: '/assets/portfolio/komodo.svg',
                            url:'https://komodoplatform.com/'
                        },
                        {
                            class: 'paypie',
                            name: 'Paypie',
                            symbol: '/assets/portfolio/paypie.svg',
                            url:'https://www.paypie.com/'
                        },
                        {
                            class: 'playkey',
                            name: 'Playkey',
                            symbol: '/assets/portfolio/playkey.svg',
                            url:'https://playkey.io/'
                        },
                        {
                            class: 'qash',
                            name: 'Qash',
