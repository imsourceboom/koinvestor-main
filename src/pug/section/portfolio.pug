                        {
                            class: 'tenx',
                            name: 'TenX',
                            symbol: '/assets/portfolio/tenx.svg',
                            url:'https://tenx.tech/'
                        },
                        {
                            class: 'windingtree ',
                            name: 'Winding Tree',
                            symbol: '/assets/portfolio/windingtree.svg',
